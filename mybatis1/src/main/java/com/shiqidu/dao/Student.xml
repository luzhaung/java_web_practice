<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.shiqidu.dao.StudentDao">
    <select id="selectById" resultType="com.shiqidu.domain.Student">
        select id,name,age,email from learnjava.student where id=#{studentId}
    </select>

    <select id="selectAll" resultType="com.shiqidu.domain.Student">
        select id,name,age,email from learnjava.student
    </select>
</mapper>